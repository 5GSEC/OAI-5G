		<eNB_serverId>1</eNB_serverId>
		<air_interface>nr</air_interface>
		<eNB_Stats>yes</eNB_Stats>
		<rt_stats_cfg>datalog_rt_stats.2x2.yaml</rt_stats_cfg>
